<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///F:/Autosar_in_depth/Autosar_Documention/AUTOSAR_ARXML_Schema/AUTOSAR_00046.xsd">
	
		<AR-PACKAGES>
			<AR-PACKAGE>
			<!--  Composition session  -->
				<SHORT-NAME>Composit_SW_Lab</SHORT-NAME>
				<ELEMENTS>
				<!-- define interfaces -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_LightControl</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Control</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_LightActuate</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Actuate</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				
				<!--  SWCs Definition -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightControl_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_LightControl</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightControl</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightActuate_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_LightActuate</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightActuate</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>ALgo_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_Algo</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightActuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_Algo</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightControl</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>
				
				<!-- Composition SWCs  -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Composite_SWC1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_Algo_Comp</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightActuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>SA_LC_SWC</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightControl_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>APP_Algo_SWC</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/ALgo_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>AS_LightControl</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composite_SWC1/SA_LC_SWC</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/LightControl_SWC/PP_LightControl</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composite_SWC1/APP_Algo_SWC</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/ALgo_SWC/RP_Algo</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>DE_LightActuate</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composite_SWC1/APP_Algo_SWC</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/ALgo_SWC/PP_Algo</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/Composite_SWC1/PP_Algo_Comp</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
				<!-- Composite SWC2 -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Composite2_SWC</SHORT-NAME>
					<PORTS>
					  <R-PORT-PROTOTYPE>
					  	<SHORT-NAME>RP_LightActuate_Comp2</SHORT-NAME>
					  	<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SRIF_LightActuate</REQUIRED-INTERFACE-TREF>
					  </R-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>Composite_inst1_SWC</SHORT-NAME>
							<TYPE-TREF DEST="COMPOSITION-SW-COMPONENT-TYPE">/Composite_SWC1</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>SA_LA_SWC</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightActuate_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>AS_LightActuate</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composite2_SWC/Composite_inst1_SWC</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Composite_SWC1/PP_Algo_Comp</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF DEST="SW-COMPONENT-PROTOTYPE">/Composite2_SWC/SA_LA_SWC</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/LightActuate_SWC/RP_LightActuate</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>	
					</CONNECTORS>
					
				</COMPOSITION-SW-COMPONENT-TYPE>
				</ELEMENTS>
			</AR-PACKAGE>
		</AR-PACKAGES>


</AUTOSAR>
